Graphics
	-always redraw canvas to clear objects (!NO SELF CLEAR!!)
	-draw objects and self with zIndex algorithm


	-save
	-move to obj.pos
	-rotate obj.rotation
	-move to obj.origin
	-flip ctx
	-apply styles
	for all obj.objs and Obj
	{
		next smallest zIndex
		if !Obj{
			-save
			-move to obj.pos
			-rotate obj.rotation
			-move to obj.origin
			-flip ctx
			-apply styles
			-draw Obj shadow
			-draw Obj,imgs,grid
			-restore
		}
		-draw Obj shadow
		-draw Obj,imgs
	}
	-restore